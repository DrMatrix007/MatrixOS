{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo xtask run",
			"type": "shell",
			"command": "cargo xtask run",
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "."
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".",
					"endsPattern": "."
				}
			},
			"presentation": {
				"reveal": "always",
				"panel": "new"
			}
		}
	]
}